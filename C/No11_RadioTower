#include <stdio.h>
int main(void)
{
    char str[1000];
    fgets(str, sizeof(str), stdin);
    fgets(str, sizeof(str), stdin);
    int i=0,successCount=0;
    while(1){
        char c=str[i++];
        if(c=='\n'){
            break;
        }
        if('6'<=c && c<='9'){
            successCount++;
        }
    }
    printf("%d", successCount);
    return 0;
}
